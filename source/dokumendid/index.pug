doctype html
html(lang='en')
    include ../head.pug
        title= 'Eesti Segakooride Liit - Dokumendid'
    body
        include ../navigation.pug

        h4 #[strong= self.title]
        table(width="100%")
            each dokument, id in self.dokumendid
                tr
                    td
                        a(href=dokument.url) #[strong= dokument.nimi]


        include ../footer.pug